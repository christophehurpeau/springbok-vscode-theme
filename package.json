{
  "name": "springbok-theme",
  "displayName": "Springbok Theme",
  "publisher": "chrp",
  "repository": "christophehurpeau/springbok-vscode-theme",
  "description": "Springbok Theme",
  "type": "module",
  "version": "0.5.0",
  "license": "MIT",
  "icon": "images/springbok-logo.png",
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Themes"
  ],
  "galleryBanner": {
    "color": "#B55A26",
    "theme": "dark"
  },
  "prettier": {
    "singleQuote": true
  },
  "contributes": {
    "themes": [
      {
        "label": "Springbok Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/SpringbokTheme-color-theme.json"
      }
    ]
  },
  "scripts": {
    "lint:prettier": "prettier --check .",
    "publish-patch": "vsce publish patch",
    "publish-minor": "vsce publish minor"
  },
  "devDependencies": {
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "prettier": "2.7.1",
    "vsce": "2.10.0"
  }
}
