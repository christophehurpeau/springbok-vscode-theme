{
  "name": "springbok-theme",
  "displayName": "Springbok Theme",
  "publisher": "chrp",
  "repository": "christophehurpeau/springbok-vscode-theme",
  "description": "Springbok Theme",
  "type": "module",
  "version": "0.9.1",
  "license": "MIT",
  "icon": "images/springbok-logo.png",
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Themes"
  ],
  "galleryBanner": {
    "color": "#B55A26",
    "theme": "dark"
  },
  "prettier": {
    "singleQuote": true
  },
  "contributes": {
    "themes": [
      {
        "label": "Springbok Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/Springbok-legacy-theme.json"
      },
      {
        "label": "Dark Springbok",
        "uiTheme": "vs-dark",
        "path": "./themes/Springbok-dark-theme.json"
      },
      {
        "label": "Light Springbok",
        "uiTheme": "vs",
        "path": "./themes/Springbok-light-theme.json"
      }
    ]
  },
  "scripts": {
    "lint:prettier": "prettier --check .",
    "publish-patch": "vsce publish patch",
    "publish-minor": "vsce publish minor"
  },
  "devDependencies": {
    "color-convert": "2.0.1",
    "@types/color-convert": "2.0.4",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "prettier": "3.5.1",
    "vsce": "2.10.0"
  },
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
}
