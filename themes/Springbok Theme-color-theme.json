{
  "name": "Springbok Theme",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#263238",
    "editor.foreground": "#eeeeff",

    "sideBar.background": "#fff",
    "sideBarTitle.foreground": "#bbbbbb",
    "statusBar.background": "#363636",
    "activityBarBadge.background": "#bc632e",
    "editorIndentGuide.activeBackground": "#abababa4"
  },
  "semanticTokenColors": {
    /* -- Values -- */
    "enumMember": {
      "fontStyle": "bold italic"
    },
    "variable.constant": {
      "bold": true
    },
    /* -- Types -- */
    "type": {
      "foreground": "#8A653B"
    },
    /* -- Entities -- */
    "class": {
      "bold": true
    },
    /* -- Default Library -- */
    "variable.defaultLibrary": {
      "foreground": "#9876AA",
      "fontStyle": "bold italic"
    },
    "function.defaultLibrary": {
      "foreground": "#bcaa2e"
    },
    "class.defaultLibrary": {
      "foreground": "#bcaa2e"
    },
    /* -- Variables -- */
    "parameter": {
      "italic": true
    },
    "*.static": {
      "bold": true
    }
  },
  "tokenColors": [
    // OK

    /* -- Values -- */

    /* Numeric */
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#62d0ff"
      }
    },

    /* String */
    {
      "scope": "string",
      "settings": {
        "foreground": "#ffa014"
      }
    },
    {
      "name": "Escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#ffd89d"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#ffd89d"
      }
    },
    {
      "scope": "meta.template.expression",
      "settings": {
        "foreground": "#eeeeff"
      }
    },

    /* Regexp */
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#C365CA"
      }
    },
    {
      "name": "Regex Character Class + Escape",
      "scope": "constant.character.escape.backslash.regexp",
      "settings": {
        "foreground": "#e5bde8"
      }
    },
    {
      "name": "Regex Group/Set",
      "scope": [
        "punctuation.definition.group.regexp",
        "punctuation.definition.character-class.regexp"
      ],
      "settings": {
        "foreground": "#f0daf2"
      }
    },

    /* Language */
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#bc632e",
        "fontStyle": "bold"
      }
    },

    /* Colors */
    {
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },

    /* Timestamp */
    {
      "scope": ["constant.other.timestamp"],
      "settings": {
        "foreground": "#ECC800"
      }
    },

    /* -- Comment -- */
    {
      "name": "Comment",
      "scope": ["comment", "comment.block"],
      "settings": {
        "foreground": "#707070"
      }
    },
    {
      "name": "Comment Documentation",
      "scope": ["comment.block.documentation"],
      "settings": {
        "foreground": "#6A8759"
      }
    },
    {
      "name": "JsDoc",
      "scope": [
        "storage.type.class.jsdoc",
        "punctuation.definition.block.tag.jsdoc"
      ],
      "settings": {
        "foreground": "#6A8759",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "JsDoc Parameter name",
      "scope": "variable.other.jsdoc",
      "settings": {
        "foreground": "#629755",
        "fontStyle": "italic"
      }
    },

    /* -- Keywords, Storage, Punctuation -- */
    {
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#bc632e",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage.modifier"],
      "settings": {
        "foreground": "#bc632e",
        "fontStyle": "bold italic"
      }
    },

    {
      "scope": ["keyword", "keyword.operator.new", "keyword.control"],
      "settings": {
        "foreground": "#bc632e"
      }
    },

    {
      "scope": ["keyword.operator", "punctuation.definition.block"],
      "settings": {
        "foreground": "#dddddd",
        "fontStyle": ""
      }
    },

    /* -- Entities -- */
    {
      "name": "Method",
      "scope": ["meta.definition.method"],
      "settings": {
        "foreground": "#FFC66D"
      }
    },

    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#cccccc"
      }
    },

    /* -- Variables -- */
    {
      "name": "Language",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#bc632e",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "variable.other.readwrite",
        "variable.other.constant"
      ],
      "settings": { "foreground": "#A9B7C6" }
    },
    {
      "scope": ["variable.other.readwrite.alias", "variable.other.alias"],
      "settings": { "foreground": "#eeeeff" }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#9876AA"
      }
    },

    /* -- Types -- */
    {
      "scope": [
        "meta.interface",
        "meta.type.declaration",
        "meta.type.annotation",
        "meta.return.type"
      ],
      "settings": {
        "foreground": "#8A653B"
      }
    },
    {
      "scope": "entity.name.type.enum",
      "settings": {
        "foreground": "#eeeeff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.type.builtin", "support.type.primitive"],
      "settings": {
        "foreground": "#ae7f4a",
        "fontStyle": "bold"
      }
    },

    /* -- Tag, Html and React -- */
    {
      "scope": ["punctuation.definition.tag", "entity.name.tag"],
      "settings": {
        "foreground": "#E8BF6A"
      }
    },
    {
      "scope": "support.class.component",
      "settings": {
        "foreground": "#eeeeff"
      }
    },

    /* -- Json/Yaml -- */
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#E8BF6A"
      }
    },

    /* -- Markdown -- */
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#bc632e",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#9876AA"
      }
    },

    // Not OK
    {
      "scope": "strong",
      "settings": {
        "foreground": "#f00",
        "fontStyle": "bold"
      }
    }
