  "semanticTokenColors": {
    "enumMember": {
      "fontStyle": "bold italic"
    },
    "variable.constant": {
      "bold": true
    },
    "type": {
      "foreground": "#88B8E2"
    },
    "interface": {
      "foreground": "#88B8E2"
    },
    "class": {
      "foreground": "#fefefe",
      "bold": true
    },
    "class.defaultLibrary": {
      "foreground": "#fefefe",
      "fontStyle": "bold"
    },
    "function": {
      "foreground": "#B488E2"
    },
    "function.defaultLibrary": {
      "foreground": "#D34F4A",
      "fontStyle": "bold"
    },
    "variable.defaultLibrary": {
      "foreground": "#D34F4A"
    },
    "parameter": {
      "italic": true
    },
    "*.static": {
      "bold": true
    }
  },
  "tokenColors": [
    {
      "scope": [
        "constant.numeric",
        "constant.numeric keyword.other.unit",
        "constant.other.timestamp"
      ],
      "settings": {
        "foreground": "#E6CF00"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#E68A00"
      }
    },
    {
      "name": "Escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#ff6a14"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#ff6a14"
      }
    },
    {
      "scope": "meta.template.expression",
      "settings": {
        "foreground": "#eeeeff"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#C365CA"
      }
    },
    {
      "name": "Regex Character Class + Escape",
      "scope": "constant.character.escape.backslash.regexp",
      "settings": {
        "foreground": "#e5bde8"
      }
    },
    {
      "name": "Regex Group/Set",
      "scope": [
        "punctuation.definition.group.regexp",
        "punctuation.definition.character-class.regexp"
      ],
      "settings": {
        "foreground": "#f0daf2"
      }
    },
    {
      "scope": [
        "constant.language"
      ],
      "settings": {
        "foreground": "#D34F4A"
      }
    },
    {
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.block"
      ],
      "settings": {
        "foreground": "#707070"
      }
    },
    {
      "name": "Comment Documentation",
      "scope": [
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#6A8759"
      }
    },
    {
      "name": "JsDoc",
      "scope": [
        "storage.type.class.jsdoc",
        "punctuation.definition.block.tag.jsdoc"
      ],
      "settings": {
        "foreground": "#6A8759",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "JsDoc Parameter name",
      "scope": "variable.other.jsdoc",
      "settings": {
        "foreground": "#629755",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#D37947",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.operator",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.logical.python",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#D37947"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#D3C64A"
      }
    },
    {
      "name": "Keys",
      "scope": [
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#B57E26"
      }
    },
    {
      "scope": [
        "entity.name.function.preprocessor"
      ],
      "settings": {
        "foreground": "#D34AC1",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#fefefe",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#B488E2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Language",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "foreground": "#D37947",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Parameter",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#ebebeb",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.other.enummember"
