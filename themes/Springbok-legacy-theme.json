      "settings": {
        "foreground": "#B5A726"
      }
    },
    {
      "scope": "support.class.component",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "meta.tag.attributes punctuation.section.embedded"
