      "settings": {
        "foreground": "#aaaabb"
      }
    },
    {
      "scope": [
        "meta.import variable.other.readwrite.alias"
      ],
      "settings": {
        "foreground": "#ddddee"
      }
    },
    {
      "scope": [
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#8bb8e0"
      }
    },
    {
      "scope": [
        "meta.definition.property"
      ],
      "settings": {
        "foreground": "#4691d2"
      }
    },
    {
      "scope": [
        "support.type.builtin",
        "support.type.primitive",
        "storage.type.built-in"
      ],
      "settings": {
        "foreground": "#8bb8e0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation.definition.tag",
        "entity.name.tag"
