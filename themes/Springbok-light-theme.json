  "semanticTokenColors": {
    "enumMember": {
      "fontStyle": "bold italic"
    },
    "variable.constant": {
      "bold": true
    },
    "type": {
      "foreground": "#347DBD"
    },
    "interface": {
      "foreground": "#347DBD"
    },
    "class": {
      "foreground": "#080808",
      "bold": true
    },
    "class.defaultLibrary": {
      "foreground": "#080808",
      "fontStyle": "bold"
    },
    "function": {
      "foreground": "#7634BD"
    },
    "function.defaultLibrary": {
      "foreground": "#6C0F0B",
      "fontStyle": "bold"
    },
    "variable.defaultLibrary": {
      "foreground": "#6C0F0B"
    },
    "parameter": {
      "italic": true
    },
    "*.static": {
      "bold": true
    }
  },
  "tokenColors": [
    {
      "scope": [
        "constant.numeric",
        "constant.numeric keyword.other.unit",
        "constant.other.timestamp"
      ],
      "settings": {
        "foreground": "#BAA804"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#BA7104"
      }
    },
    {
      "name": "Escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#ff6a14"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#ff6a14"
      }
    },
    {
      "scope": "meta.template.expression",
      "settings": {
        "foreground": "#111100"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#C365CA"
      }
    },
    {
      "name": "Regex Character Class + Escape",
      "scope": "constant.character.escape.backslash.regexp",
      "settings": {
        "foreground": "#e5bde8"
      }
    },
    {
      "name": "Regex Group/Set",
      "scope": [
        "punctuation.definition.group.regexp",
        "punctuation.definition.character-class.regexp"
